{
  "name": "dual-build",
  "version": "0.0.101",
  "author": "Franz Zemen",
  "type": "module",
  "exports": {
    "./project": {
      "types": "./out/dist/types/index.d.ts",
      "import": "./out/dist/esm/index.js"
    },
    "./build": {
      "types": "./out/build/index.d.ts",
      "import": "./out/build/index.js"
    }
  },
  "imports": {
    "#project": "./out/dist/esm/index.js"
  },
  "scripts": {
    "setup": "npx tsc -b && node bin/build",
    "compile": "npx tsc -b",
    "build": "npx tsc -b && node bin/build",
    "check-in": "npx tsc -b && node bin/check-in",
    "push": "npx tsc -b && node bin/push",
    "publish": "npx tsc -b && node bin/publish",
    "clean": "node bin/clean",
    "test": "mocha transient/test/**/*.test.js"
  },
  "nodemonConfig": {
    "exec": "npx mocha ./transient/test/**/*.js",
    "watch": [
      "./transient/**/*"
    ],
    "delay": 100,
    "env": {
      "NODE_ENV": "test"
    }
  },
  "dependencies": {
    "fast-glob": "^3.2.12",
    "fastest-validator": "^1.16.0",
    "inquirer": "^9.1.4",
    "inquirer-autocomplete-prompt": "^3.0.0",
    "is-stream": "^3.0.0",
    "lodash": "^4.17.21",
    "simple-git": "^3.16.0",
    "spdx-license-ids": "^3.0.12",
    "tsconfig.d.ts": "^0.1.20",
    "uuid": "^9.0.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/inquirer": "^9.0.3",
    "@types/inquirer-autocomplete-prompt": "^3.0.0",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.19",
    "@types/uuid": "^9.0.0",
    "@types/yargs": "^17.0.22",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "ts-to-jsdoc": "^1.2.1",
    "typescript": "^4.9.5"
  }
}
